{
  "expo": {
    "name": "jetflix",
    "slug": "jetflix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./packages/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./packages/assets/images/Jetflix.png",
      "resizeMode": "contain",
      "backgroundColor": "black"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./packages/assets/images/adaptive-icon.png",
        "backgroundColor": "black"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./packages/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
